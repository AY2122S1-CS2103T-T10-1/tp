@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":ExportCommandParser" as ExportCommandParser LOGIC_COLOR
participant "e:ExportCommand" as ExportCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Storage STORAGE_COLOR_T1
participant "temporaryStorage:JsonAddressBookStorage" as JsonAddressBookStorage STORAGE_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("export newfriends.json")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("export newfriends.json")
activate AddressBookParser

create ExportCommandParser
AddressBookParser -> ExportCommandParser
activate ExportCommandParser

ExportCommandParser --> AddressBookParser
deactivate ExportCommandParser

AddressBookParser -> ExportCommandParser : parse("newfriends.json")
activate ExportCommandParser

create ExportCommand
ExportCommandParser -> ExportCommand
activate ExportCommand

ExportCommand --> ExportCommandParser : e
deactivate ExportCommand

ExportCommandParser --> AddressBookParser
deactivate ExportCommandParser

AddressBookParser --> LogicManager : e
deactivate AddressBookParser

LogicManager -> ExportCommand : execute()
activate ExportCommand

ExportCommand -> Model : getAddressBook()
activate Model

Model --> ExportCommand : currentAddressBook
deactivate Model

create JsonAddressBookStorage
ExportCommand -> JsonAddressBookStorage
activate JsonAddressBookStorage

JsonAddressBookStorage --> ExportCommand
deactivate JsonAddressBookStorage

ExportCommand -> JsonAddressBookStorage : exportToJson()
activate JsonAddressBookStorage

JsonAddressBookStorage --> ExportCommand
deactivate JsonAddressBookStorage

create CommandResult
ExportCommand -> CommandResult
activate CommandResult

CommandResult --> ExportCommand : result
deactivate CommandResult

ExportCommand --> LogicManager : result
deactivate ExportCommand

[<--LogicManager

@enduml
